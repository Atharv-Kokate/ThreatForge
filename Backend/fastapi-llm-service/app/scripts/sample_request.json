{
  "product": {
    "name": "MediAssist AI",
    "description": "An AI assistant for doctors that summarizes patient medical records and suggests potential diagnoses based on symptoms.",
    "category": "healthcare-application",
    "technology": "Python, LangChain, OpenAI GPT-4, Vector DB",
    "version": "2.1.0"
  },
  "questionnaire": {
    "applicationContext": {
      "systemType": "agentic-workflow",
      "domain": "healthcare",
      "criticality": "safety-critical"
    },
    "dataHandling": {
      "sources": [
        "electronic-health-records",
        "medical-journals"
      ],
      "containsSensitive": [
        "phi",
        "medical-history"
      ],
      "sanitizeBeforeModelUse": true
    },
    "modelDetails": {
      "modelType": "foundation-api",
      "maintenance": "continuous",
      "visibility": "black-box"
    },
    "systemArchitecture": {
      "deployment": "hybrid-cloud",
      "access": [
        "internal-api",
        "vpn-only"
      ],
      "integrations": [
        "ehr-system",
        "email-notifications"
      ]
    },
    "interactionControl": {
      "inputs": [
        "structured-data",
        "free-text-notes"
      ],
      "outputs": [
        "clinical-summary",
        "decision-support"
      ],
      "promptGuardrails": true
    },
    "securityPractices": {
      "auth": [
        "mfa",
        "sso"
      ],
      "logging": "comprehensive",
      "encryption": true
    },
    "threatSurface": {
      "multiTenant": true,
      "externalQuery": false,
      "adversarialProtection": true
    },
    "complianceGovernance": {
      "frameworks": [
        "hipaa",
        "gdpr",
        "hitrust"
      ],
      "explainability": false,
      "retentionPolicies": true
    }
  },
  "analysis": {},
  "context": {},
  "metadata": {
    "model": "gpt-4",
    "test": true
  }
}